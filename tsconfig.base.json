{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "composite": true,
    "declaration": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "noErrorTruncation": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "ESNext"
  }
}
